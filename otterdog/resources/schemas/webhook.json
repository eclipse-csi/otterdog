{
  "$schema": "https://json-schema.org/draft/2020-12/schema",

  "type": "object",
  "properties": {
    "active": { "type": "boolean" },
    "aliases": {
      "type": "array",
      "items": { "type": "string" }
    },
    "events": {
      "type": "array",
      "items": { "type": "string" }
    },
    "url": { "type": "string" },
    "content_type": { "type": "string" },
    "insecure_ssl": { "type": "string" },
    "secret": { "$ref": "types.json#/$defs/string-or-null"}
  },

  "required": [ "active" ],
  "additionalProperties": false
}
